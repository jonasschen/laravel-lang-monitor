{
    "name": "jonasschen/laravel-lang-monitor",
    "description": "Search for untranslated keys in your Laravel project",
    "keywords": [
        "jonasschen",
        "laravel-lang-monitor",
        "laravel-lang",
        "lang-monitor",
        "translation",
        "language",
        "lang",
        "i18n",
        "translation"
    ],
    "type": "library",
    "homepage": "https://github.com/jonasschen/laravel-lang-monitor",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jonasschen\\LaravelLangMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonasschen\\LaravelLangMonitor\\Tests\\": "tests"
        },
        "classmap": [
            "tests"
        ]
    },
    "authors": [
        {
            "name": "Jonas Schen",
            "email": "jonasschen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.44",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "Jonasschen\\LaravelLangMonitor\\Providers\\LangMonitorServiceProvider"
            ],
            "aliases": {
                "LaravelLangMonitor": "Jonasschen\\LaravelLangMonitor\\Facades\\LangMonitorFacade"
            }
        }
    }
}
